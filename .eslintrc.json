{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb", "plugin:sonarjs/recommended"],
    "ignorePatterns": ["/gulp-tasks", "gulpfile.babel.js"],
    "plugins": ["sonarjs"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    }
}
